          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "Listagem de usuarios"
          }
        ],
        "responses": {
          "200": {
            "description": "Listagem de usuarios",
            "content": {
              "application/json": {
                "example": {
                  "usuarios": [
                    {
                      "email": "shaolin@gmail.com",
                      "senha": "aArFha251",
                      "celular": "(11)9 9999-9999",
                      "usuarioId": 1
                    }
                  ],
                  "meta": {
                    "pagination": {
                      "total": 100,
                      "per_page": 25,
                      "current_page": 1,
                      "last_page": 4,
                      "next_page_url": "/usuarios?page=2",
                      "prev_page_url": null,
                      "from": 1,
                      "to": 25
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Usuarios nao encontrado"
          }
        }
      }
    },
    "/cartoes/{id}": {
      "get": {
        "summary": "Listar cartao",
