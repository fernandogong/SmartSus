  "paths": {
    "/clinicas": {
      "post": {
        "summary": "Cadastrar clinica",
        "responses": {
          "201": {
            "description": "Clinica cadastrada com sucesso"
          },
          "400": {
            "description": "Campos invalidos"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "nome": {
                    "type": "string"
                  },
                  "especialidade": {
                    "type": "string"
                  },
                  "cnpj": {
                    "type": "string"
                  },
                  "preco": {
                    "type": "number"
                  },
                  "clinicaId": {
                    "type": "integer"
                  }
                },
                "required": ["nome", "especialidade", "cnpj", "preco", "clinicaId"]
              },
              "example": {
                "nome": "FIAP CLINICA",
                "especialidade": "cardiologista",
                "cnpj": "61.282.051/0001-48",
                "preco": 98.99,
                "clinicaId": 1
              }
            }
          }
        }
      },
     "get": {
      "summary": "Listar clinicas",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          },
          "description": "Listagem de clinicas"
        }
      ],
      "responses": {
        "200": {
          "description": "Lista de clinicas",
          "content": {
            "application/json": {
              "example": {
                "clinicas": [
                  {
                    "nome": "FIAP CLINICA",
                    "especialidade": "cardiologista",
                    "cnpj": "61.282.051/0001-48",
                    "preco": 98.99,
                    "clinicaId": 1
                  }
                ],
                "meta": {
                  "pagination": {
                    "total": 100,
                    "per_page": 25,
                    "current_page": 1,
                    "last_page": 4,
                    "next_page_url": "/clinicas?page=2",
                    "prev_page_url": null,
                    "from": 1,
                    "to": 25
                  }
                }
              }
            }
          }
        },
        "404": {
          "description": "Clinicas nao encontradas"
    },
    "delete": {
      "summary": "Apagar clinica",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          },
          "description": "ID da clinica a ser apagada"
        }
      ],
      "responses": {
        "204": {
          "description": "Clinica apagada com sucesso"
        },
        "404": {
          "description": "Clinica nao encontrada"
        }
      }
    },
    "/clinicas/{id}": {
      "get": {
        "summary": "Exibir uma clinica",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID da clinica a ser buscada"
          }
        ],
        "responses": {
          "200": {
            "description": "Exibir uma clinica",
            "content": {
              "application/json": {
                "example": {
                 "clinicas": [
                    {
                      "nome": "FIAP CLINICA",
                      "especialidade": "cardiologista",
                      "cnpj": "61.282.051/0001-48",
                      "preco": 98.99,
                      "clinicaId": 1
                    }
                  ],
                  "meta": {
                    "pagination": {
                      "total": 100,
                      "per_page": 25,
                      "current_page": 1,
                      "last_page": 4,
                      "next_page_url": "/clinicas?page=2",
                      "prev_page_url": null,
                      "from": 1,
                      "to": 25
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Clinica nao encontrada"
          }
        }
      },
      "put": {
        "summary": "Atualizar clinica",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "ID da clinica a ser atualizada"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "nome": {
                    "type": "string"
                  },
                  "especialidade": {
                    "type": "string"
                  },
                  "cnpj": {
                    "type": "string"
                  },
                  "preco": {
                    "type": "number"
                  },
                  "clinicaId": {
                    "type": "integer"
                  }
                },
                "required": ["nome", "especialidade", "cnpj", "preco", "clinicaId"]
              },
              "example": {
                "nome": "FIAP CLINICA",
                "especialidade": "Cirurgiao geral",
                "cnpj": "61.282.051/0001-48",
                "preco": 209.99,
                "clinicaId": 1
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Dados foram alterados com sucesso"
          },
          "400": {
            "description": "Campos invalidos para alteracao"
          },
          "404": {
            "description": "Clinica nao encontrada"
          }
        }
      }
    }
  }
