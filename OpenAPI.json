                  "usuarioId": {
                    "type": "integer"
                  },
                  "clinicaId": {
                    "type": "integer"
                  }
                },
                "required": ["data", "agendamentoId", "usuarioId", "clinicaId"]
              },
              "example": {
                "data": "01-01-2024 12:00:00",
