    "/cartoes": {
      "post": {
        "summary": "Cadastrar cartao",
        "responses": {
          "201": {
            "description": "Cartão cadastrado com sucesso"
          },
          "400": {
            "description": "Campos inválidos"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "numero": {
                    "type": "string"
                  },
                  "nome": {
                    "type": "string"
                  },
                  "validade": {
                    "type": "string",
                    "format": "date"
                  },
                  "cvv": {
                    "type": "integer"
                  },
                  "cpf": {
                    "type": "string"
                  },
                  "usuarioId": {
                    "type": "integer"
                  },
                  "cartaoId": {
                    "type": "integer"
                  }
                },
                "required": ["numero", "nome", "validade", "cvv", "cpf", "usuarioId", "cartaoId"]
              },
              "example": {
                "numero": "1234 5678 1234 5678",
                "nome": "Carthanos",
                "validade": "01/27",
                "cvv": 123,
                "cpf": "123.456.789-10",
                "usuarioId": 1,
                "cartaoId": 1
              }
            }
          }
        }
      }
      "get": {
        "summary": "Listar usuarios",
